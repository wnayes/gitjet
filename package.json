{
  "name": "gitjet",
  "version": "1.0.0",
  "description": "Fast git log viewer",
  "main": "./src/main/dist.js",
  "scripts": {
    "build": "npm run build-main && npm run build-preload && npm run build-renderer",
    "build-main": "esbuild ./src/main/main.ts --bundle --sourcemap --format=cjs --packages=external --platform=node --outfile=./src/main/dist.js",
    "build-preload": "esbuild ./src/preload/preload.ts --bundle --sourcemap --format=cjs --packages=external --platform=node --outfile=./src/preload/dist.js",
    "build-renderer": "esbuild ./src/renderer/log/index.tsx --bundle --sourcemap --format=esm --outfile=./src/renderer/log/dist.js",
    "start": "electron ."
  },
  "repository": "https://github.com/wnayes/gitjet",
  "keywords": [
    "git",
    "log"
  ],
  "author": "Will Nayes",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "electron": "^29.1.1",
    "esbuild": "^0.20.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-resizable-panels": "^2.0.13",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^1.0.9",
    "zustand": "^4.5.2"
  }
}
